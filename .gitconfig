[filter "media"]
	required = true
	clean = git media clean %f
	smudge = git media smudge %f
[user]
	name = Seb Jacobs
	email = me@sebjacobs.com
[core]
  excludesfile = ~/.gitignore
[branch "master"]
[color]
  status = auto
  branch = auto
  diff = auto
  interactive = auto
[push]
  default = simple
[log]
  abbrevCommit = true
[alias]
	l = "log --oneline --graph --decorate"
	lp = "log --oneline --graph --decorate --pretty"
	la = "log --oneline --graph --decorate --all"
	praise = blame
[github]
  user = sebjacobs
[git-pair]
  pattern = pairing+%name+%name@futurelearn.com
	authors = sebjacobs <me@sebjacobs.com>
	authors = mseckington <mseckington@gmail.com>
	authors = nicky <nicky.thompson@gmail.com>
	authors = zetter <zetter@gmail.com>
	authors = eightbitraptor <matt@eightbitraptor.com>
	authors = jcoglan <jcoglan@gmail.com>
	authors = wannabefro <sam.mctaggart@gmail.com>
	authors = alssndro <alex.mura@futurelearn.com>
	authors = mrageh <adam.magan@futurelearn.com>
	authors = ohthatjames <ohthatjames@gmail.com>
	authors = urbanautomaton <simon@urbanautomaton.com>
	authors = daniel-ferguson <danielferguson@me.com>
	authors = jsphmrrw <joseph.morrow@futurelearn.com>
	authors = mattbee <matt.bee@futurelearn.com>
	authors = marcotranchino <marco.tranchino@futurelearn.com>
[url "git@github.com:"]
	insteadOf = https://github.com/
[url "git@bitbucket.org:"]
	insteadOf = https://bitbucket.org/
